{
  "name": "public-page",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/public-page/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "defaultConfiguration": "production",
      "cache": true,
      "options": {
        "outputPath": "dist/apps/public-page",
        "configFile": "apps/public-page/vite.config.ts"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "public-page:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "public-page:build:development"
        },
        "production": {
          "buildTarget": "public-page:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "cache": true
    }
  }
}
